{% extends 'SiteMainBundle:Frontend/Auth:dialog.html.twig' %}

{% block id %}payment-dialog{% endblock %}

{% block head %}Оплата{% endblock %}

{% block content %}
    <div class="wrap-form">
        <form action=""></form>
        <div class="form payment-form">
            {{ form_start(form, { 'style': 'horizontal', attr: {novalidate: 'novalidate'} }) }}
            {{ form_row(form.tariff) }}
            {{ form_row(form.numberMonth) }}
            {{ form_widget(form.submit) }}
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}